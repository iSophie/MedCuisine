                            <fo:block-container font-size="2em" space-after="1em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title5"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 3 - <!--content(text)--><fo:inline>IBM StoredIQ components</fo:inline></fo:marker><fo:inline>Chapter 3. </fo:inline>IBM
                                StoredIQ components</fo:block></fo:block-container>
                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The IBM StoredIQ solution consists of these components: the
                                    application stack, the gateway, the data server, and optionally
                                    the Elasticsearch cluster.</fo:block></fo:block-container>
                            </fo:block></fo:block-container>
                            <fo:block id="solution_components" font-size="0" keep-with-next="always"/><fo:block role="Sect" id="unique_6">
                                <fo:block id="d30e483"/>
                                <fo:block id="d30e487"/>
                                <fo:block id="d30e492"/>
                                <fo:block id="d30e496"/>
                                <fo:block id="d30e501"/>
                                <fo:block id="d30e506"/>
                                <fo:block id="d30e511"/>
                                <fo:block id="d30e517"/>
                                <fo:block id="d30e521"/>
                                <fo:block id="d30e526"/>
                                <fo:block id="d30e531"/>
                                <fo:block id="d30e535"/>
                                <fo:block id="d30e540"/>
                                <fo:block id="d30e545"/>
                                <fo:block id="d30e550"/>
